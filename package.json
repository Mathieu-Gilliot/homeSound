{
  "name": "home-sound",
  "version": "1.0.7",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mathieu-Gilliot/homeSound"
  },
  "description": "Your own local music player",
  "author": {
    "name": "Gilliot Mathieu",
    "email": "mathieu.gilliot.pro@gmail.com"
  },
  "build": {
